{
  "network": {
    "architecture": "resnet34",
    "weights_dir": "weights",
    "optimiser": "Adam",
    "learning_rate": 1e-4,
    "epoch_every_train": 1,
    "start_from": "begin" // "begin" - start from stddev weights, "url" - start from weights, loaded by url, "last" - load last weights from workdir
  },
  "data_conveyor": {
    "dataset_path": {"folders": {"train": "train", "validation": "validation"}},
    "data_size": [256, 256, 3],
    "batch_size": 32
  },
  "workdir_path": "workdir"
}